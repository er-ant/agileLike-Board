{
  "extends": "eslint-config-airbnb-base/legacy",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "max-len": ["error", 100],
    "no-param-reassign": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  },
  "parser": "babel-eslint"
}

